x = 100
y = 20
hight = 8
point_back = 0

turtle.select(1)
--turtle.equipLeft()

function forward_dig()
  for i = 0,x,1 do
    turtle.dig()
    turtle.forward()
  end
end

--programs
for downcount = 0,hight,1 do
  for i = 0,y,1 do
    forward_dig()
    turtle.turnLeft()
    turtle.dig()
    turtle.forward()
    turtle.turnLeft()
    forward_dig()
    turtle.turnRight()
    turtle.dig()
    turtle.forward()
    turtle.turnRight()
  end
  turtle.turnRight()
  
  for point_back = 0,i+1,1 do
    turtle.forward()
  end
  turtle.turnLeft()
  turtle.digDown()
  turtle.down()
end

turtle.up()

for i = 0,hight,1 do
 turtle.up()
end

