wood_hight = 3
wood_length = 3

for i = 1,wood_length,1 do
  turtle.dig()
  turtle.forward()
  
    for j = 1,wood_hight,1 do
      turtle.digDown()
      turtle.down()
    end
  
    for k = 1,wood_hight,1 do
      turtle.up()
    end
end

turtle.turnLeft()
turtle.turnLeft()

for i = 1,wood_length,1 do
  turtle.forward()
end

turtle.turnLeft()
turtle.turnLeft()
