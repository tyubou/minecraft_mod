wood_cut_length = 0

function ninety_ninety()
  turtle.turnLeft()
  turtle.turnLeft()
end

function forward_cut(number)
  for i = 0,number,1 do
    turtle.dig()
    turtle.forward()
  end
end

function reverse_forward(number)
  ninety_ninety()
  for i = 0,number,1 do
    turtle.dig()
    turtle.forward()
  end
end

function five_wood_cutter()


end


forward_cut(5)
reverse_forward(5)
